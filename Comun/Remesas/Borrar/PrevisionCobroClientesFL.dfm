  object QEstadoCobros: TQuery
    DatabaseName = 'BDProyecto'
    SQL.Strings = (
      'SELECT n_factura_f, fecha_factura_f, importe_total_f, moneda_f,'
      '       importe_euros_f, importe_cobrado_f, remesa_f,'
      ''
      '       ( select min(fecha_r) '
      '         from frf_remesas'
      '         where empresa_r = :empresa'
      '           and referencia_r = remesa_f'
      '           and banco_r = banco_f'
      '           and fecha_r >= fecha_factura_f ) fecha_r,'
      ''
      '       contabilizado_f, contab_cobro_f,'
      
        '       (CASE WHEN (importe_cobrado_f=0 OR importe_cobrado_f IS N' +
        'ULL)'
      '             THEN  0            '
      '             ELSE (importe_cobrado_f - importe_euros_f)'
      '        END) diferencia'
      'FROM frf_facturas'
      'WHERE empresa_f = :empresa'
      'AND   cliente_fac_f = :cliente'
      'AND   fecha_factura_f BETWEEN :desde AND :hasta'
      'ORDER BY fecha_factura_f, n_factura_f'
      ' '
      ' '
      ' ')
    Left = 392
    Top = 255
    ParamData = <
      item
        DataType = ftString
        Name = 'empresa'
        ParamType = ptInput
      end
      item
        DataType = ftString
        Name = 'empresa'
        ParamType = ptInput
      end
      item
        DataType = ftString
        Name = 'cliente'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'desde'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'hasta'
        ParamType = ptInput
      end>
  end
